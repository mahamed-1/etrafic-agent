{
  "expo": {
    "name": "PSR Mobile",
    "slug": "psr-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic_launcher.png",
    "scheme": "psrapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#2563eb",
    "primaryColor": "#2563eb",
    "newArchEnabled": true,
    "android": {
      "package": "com.hypercube.psrmobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher_foreground.png",
        "backgroundColor": "#2563eb"
      },
      "permissions": []
    },
    "ios": {
      "bundleIdentifier": "com.votresociete.psrmobile",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Description d'utilisation"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ],
      "expo-secure-store"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "55c338b0-8bc6-42b6-a5a4-22ad2ecdd7e8"
      }
    }
  }
}
